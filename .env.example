# MongoDB Connection
# Local MongoDB
MONGODB_URI=mongodb://localhost:27017/image-evaluation

# Or MongoDB Atlas (Cloud)
# MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/image-evaluation?retryWrites=true&w=majority

# Environment
NODE_ENV=development

# OpenAI API Key
OPENAI_API_KEY=your_openai_api_key_here

# Google Gemini API Key
GEMINI_API_KEY=your_gemini_api_key_here

# LLM Provider Priority (comma-separated: openai,gemini)
LLM_PROVIDERS=openai,gemini
